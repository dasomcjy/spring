package com.spring.board;

public class BoardDTO {

	//DTO (VO) : DB의 테이블의 각 컬럼 이름을 매핑
		// 데이터를 받아서 전달해주는 중간자 역할 
		// 모든 변수는 private 로 선언
		// getter/setter를 사용해서 변수의 값을 할당, 가지고 옴
		// 기본 생성자를 반드시 생성함 , BoardDTO() {}
		// toString 메소드를 재정의 : 객체 자체를 출력했을때 그 객체의 변수값을 확인
		// lombok을 사용하면, 어노테이션을 사용해서 getter/setter, toString, 기본생성자를 자동으로 만들어준다.
	
	private int seq;
	private String title; 
	private String writer;
	private String content;
	private Date regdate;
	private int cnt;
	
}
